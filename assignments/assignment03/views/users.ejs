<!DOCTYPE html>
<html lang="en">
	<%- include('./includes/head.ejs') %>
	<body>
		<%- include('./includes/header.ejs') %>
		<main>
			<% if (users.length> 0) { %>
			<h1>Users List:</h1>
			<ul>
				<% users.sort().forEach(user=> { %>
				<li><%= user %></li>
				<% })} else { %>
			</ul>
			<h1>No users added</h1>
			<% } %>
		</main>
	</body>
</html>
